{
  "name": "catppuccin-vscord",
  "type": "module",
  "version": "0.0.0",
  "description": "ðŸš¦ Soothing pastel icon theme for VSCord",
  "contributors": [
    "prazdevs",
    "xhayper"
  ],
  "license": "MIT",
  "repository": "https://github.com/catppuccin/vscord",
  "keywords": [
    "catppuccin",
    "theme",
    "fancy",
    "pastel-colors",
    "vscord"
  ],
  "scripts": {
    "build": "yarn build:themes && yarn build:images && yarn build:preview",
    "build:icons": "tsx scripts/icons.ts",
    "build:images": "tsx scripts/images.ts",
    "build:themes": "tsx scripts/themes.ts",
    "build:preview": "tsx scripts/preview.ts",
    "catwalk": "tsx scripts/catwalk.ts",
    "dev": "chokidar \"src/**/*\" -c \"yarn build\"",
    "integrity": "tsx scripts/integrity.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp -t \"v%s\" -c \"chore: release v\""
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@catppuccin/palette": "^0.1.6",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.14.0",
    "bumpp": "^9.0.0",
    "chokidar-cli": "^3.0.0",
    "colorsea": "^1.1.0",
    "consola": "^2.15.3",
    "eslint": "^8.34.0",
    "fs-extra": "^11.1.0",
    "pathe": "^1.1.0",
    "puppeteer": "^19.7.2",
    "scule": "^1.0.0",
    "svgo": "^3.0.2",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  }
}
